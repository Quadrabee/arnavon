{
  "presets": [
    [
      "@babel/preset-env",
      {
        "useBuiltIns": "usage", // alternative mode: "entry"
        "corejs": 3 // default would be 2
      }
    ],
    "@babel/preset-typescript",
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-proposal-private-methods"
  ],
  "sourceMap": "inline",
  "retainLines": true
}
